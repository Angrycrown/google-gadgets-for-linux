prefix=@GGL_PREFIX@
exec_prefix=@GGL_EXEC_PREFIX@
libdir=@GGL_LIBDIR@
includedir=@GGL_INCLUDEDIR@
datarootdir=@GGL_DATAROOTDIR@
datadir=@GGL_DATADIR@
sysconfdir=@GGL_SYSCONFDIR@

ggl_module_dir=@GGL_MODULE_DIR@
ggl_resource_dir=@GGL_RESOURCE_DIR@
ggl_include_dir=@GGL_INCLUDE_DIR@
ggl_sysdeps_include_dir=@GGL_SYSDEPS_INCLUDE_DIR@
ggl_libexec_dir=@GGL_LIBEXEC_DIR@

binary_version=@LIBGGADGET_BINARY_VERSION@

Name: libggadget
Description: Core library of Google Gadgets Runtime
Version: @GGL_VERSION@
Requires:
Libs: -L${libdir} -lggadget@GGL_EPOCH@
Cflags: -I${ggl_include_dir} -I${ggl_sysdeps_include_dir} -D__STDC_CONSTANT_MACROS
