<customize>
  <UserPref datatype="enum" display_name="Plant Type" name="plant" default_value="orchid">
    <EnumValue display_value="Orchid" value="orchid" image="plant_orchid.jpg" />
    <EnumValue display_value="Tulip" value="tulip" image="plant_tulip.jpg" />
    <EnumValue display_value="Geranium" value="geranium" image="plant_geranium.jpg" />
    <EnumValue display_value="Cactus" value="cactus" image="plant_cactus.jpg" />
  </UserPref>
  <UserPref datatype="enum" display_name="Pot Type" name="pot" default_value="1">
    <EnumValue display_value="Type 1" value="1" image="pot1.jpg" />
    <EnumValue display_value="Type 2" value="2" image="pot2.jpg" />
    <EnumValue display_value="Type 3" value="3" image="pot3.jpg" />
    <EnumValue display_value="Type 4" value="4" image="pot4.jpg" />
  </UserPref>
  <UserPref datatype="enum" display_name="Color" name="color" default_value="p">
    <EnumValue display_value="Pink" value="p" image="color_pink.jpg" />
    <EnumValue display_value="Purple" value="pu" image="color_purple.jpg" />
    <EnumValue display_value="Red" value="r" image="color_red.jpg" />
    <EnumValue display_value="Yellow" value="y" image="color_yellow.jpg" />
  </UserPref>
</customize>
